<div class="navbar">
	<div class='destination'>
		<a class='logo-holder' href='../'>
          <svg class='logo' version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="32" height="32" viewBox="0 0 32 32">
			<path class="path1" d="M27.786 5.618c0.236-0.301 0.28-0.711 0.113-1.055s-0.517-0.563-0.899-0.563h-22c-0.383 0-0.732 0.219-0.899 0.563s-0.123 0.754 0.113 1.055l9.786 12.455v11.927h-3c-0.552 0-1 0.448-1 1s0.448 1 1 1h10c0.552 0 1-0.448 1-1s-0.448-1-1-1h-3v-11.927l9.786-12.455zM24.943 6l-3.143 4h-11.599l-3.143-4h17.885z"></path>
          </svg>
          <div class='logo-name'>Party-Party</div>
        </a>
	</div>
	<div class='destination'>
		<% if(userInfo) { %>
			<div class='user-interface'>
				<div class="user-info">
					<img src=<%- userInfo['profile_image_url_https'] %> class='user-image'>
				</div>
				<div class="dropdown-arrow"></div>
				<div class="dropdown-arrow-shadow"></div>
				<div class="dropdown">
					<div class='username'><%= userInfo['screen_name'] %></div>
					<a href='../sign-out'>Sign Out</a>
				</div>
			</div>
		<% } else { %>
			<div class="sign-in">
				<a href='../request-token'>Sign in</a>
			</div>
		<% } %>
	</div>
</div>